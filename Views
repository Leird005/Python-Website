from django.shortcuts import render_to_response

from blog.models import posts

def home(request):
    content = {
        'title' : 'My First Post',
        'author' : 'Leirdahl',
        'date' : '9th June 2016',
        'body' : 'This is my first blog post!',
    }

  return render_to_response('index.html', content)
